<view class="base">
  <index-bar
    className="indexbar"
    items="{{items}}"
    defaultCurrent="A"
    onChange="onChange">
    <view slot-scope="props">
      <list header="{{props.value.label}}">
        <list-item
          a:for="{{4}}"
          a:for-item="itemX">
          {{props.value.label}}-{{itemX}}
        </list-item>
      </list>
    </view>
  </index-bar>
</view>
