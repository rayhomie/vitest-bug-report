<ant-container title="受控模式">
  <ant-uploader
    fileList="{{ fileList }}"
    bind:change="onChange"
    onUpload="{{ onUpload ? onUpload : 'onUpload' }}" />
</ant-container>
<ant-container title="手动触发上传">
  <ant-uploader
    bind:ref="handleUploaderRef"
    fileList="{{ fileList }}"
    bind:change="onChange"
    onUpload="{{ onUpload ? onUpload : 'onUpload' }}" />
  <ant-button
    size="medium"
    bind:tap="upload"
    type="primary">
    上传
  </ant-button>
</ant-container>
